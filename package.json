{
    "name": "vsc-material-theme-icons",
    "displayName": "Material Theme Icons",
    "description": "Material Theme Icons, the most epic icons theme for Visual Studio Code and Material Theme.",
    "version": "4.0.0",
    "publisher": "kd3n1z",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Themes"
    ],
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "keywords": [
        "icons",
        "theme",
        "file icons",
        "file-icons",
        "icon theme",
        "icon-theme",
        "material icons",
        "material-icons"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/kd3n1z/vsc-material-theme-icons.git"
    },
    "bugs": {
        "url": "https://github.com/kd3n1z/vsc-material-theme-icons/issues"
    },
    "author": "Denis Komarkov",
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        },
        "virtualWorkspaces": true
    },
    "contributes": {
        "iconThemes": [
            {
                "id": "vsc-material-theme-icons-default",
                "label": "Material Theme Icons",
                "path": "./dist/variations/default.json"
            },
            {
                "id": "vsc-material-theme-icons-default-arrows",
                "label": "Material Theme Icons (with arrows)",
                "path": "./dist/variations/default-arrows.json"
            },
            {
                "id": "vsc-material-theme-icons-darker",
                "label": "Material Theme Icons Darker",
                "path": "./dist/variations/darker.json"
            },
            {
                "id": "vsc-material-theme-icons-darker-arrows",
                "label": "Material Theme Icons Darker (with arrows)",
                "path": "./dist/variations/darker-arrows.json"
            },
            {
                "id": "vsc-material-theme-icons-light",
                "label": "Material Theme Icons Light",
                "path": "./dist/variations/light.json"
            },
            {
                "id": "vsc-material-theme-icons-light-arrows",
                "label": "Material Theme Icons Light (with arrows)",
                "path": "./dist/variations/light-arrows.json"
            },
            {
                "id": "vsc-material-theme-icons-palenight",
                "label": "Material Theme Icons Palenight",
                "path": "./dist/variations/palenight.json"
            },
            {
                "id": "vsc-material-theme-icons-palenight-arrows",
                "label": "Material Theme Icons Palenight (with arrows)",
                "path": "./dist/variations/palenight-arrows.json"
            },
            {
                "id": "vsc-material-theme-icons-ocean",
                "label": "Material Theme Icons Ocean",
                "path": "./dist/variations/ocean.json"
            },
            {
                "id": "vsc-material-theme-icons-ocean-arrows",
                "label": "Material Theme Icons Ocean (with arrows)",
                "path": "./dist/variations/ocean-arrows.json"
            },
            {
                "id": "vsc-material-theme-icons-deepforest",
                "label": "Material Theme Icons Deepforest",
                "path": "./dist/variations/deepforest.json"
            },
            {
                "id": "vsc-material-theme-icons-deepforest-arrows",
                "label": "Material Theme Icons Deepforest (with arrows)",
                "path": "./dist/variations/deepforest-arrows.json"
            }
        ]
    },
    "scripts": {
        "build": "node scripts/build.js"
    },
    "devDependencies": {
        "svgo": "^3.3.2"
    }
}